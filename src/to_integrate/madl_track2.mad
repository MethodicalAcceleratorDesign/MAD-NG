local function solenoid_track (elm, m)
--  io.write('SOL_TRACK: for ', elm.name, ' (l=', elm.l,')\n')
  local l, angle, tilt in elm
  if abs(angle) >= minang then
    error("invalid strait element angle (too curved): " .. elm.name)
