-- locals ---------------------------------------------------------------------o

local vector, matrix                                             in MAD
local is_nil, is_number, is_sequence, is_beam                    in MAD.typeid
local abs, sqrt, max, sin, cos, tan, asin, acos, atan, atan2,
      sinc, fact                                                 in MAD.gmath
