    end
  end
  for _,z in ipairs(unitCircle( 1, values.rad)) do
      assertAlmostEquals( (z:tan() - z:sin() / z:cos() ):real(), 0,   eps )
      assertAlmostEquals( (z:tan() - z:sin() / z:cos() ):imag(), 0,   eps )
