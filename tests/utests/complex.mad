  assertNaN   ( complex( inf, nan):acosh().im      )
  assertEquals( complex(-inf, nan):acosh().re, inf )
  assertNaN   ( complex(-inf, nan):acosh().im      )
  assertNaN   ( complex( nan, inf):acosh().im      )
