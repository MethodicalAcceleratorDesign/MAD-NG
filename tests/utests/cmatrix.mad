    end
    --'in' place test
    for d=1,#ir do
      cm:map2(ir[d], '~'     , 'in') for j=1,mn do assertEquals( cm:geti(j), j*((-1)^d)*(1+1i) ) end
    end
      cm:map2(cm   , \x,y x-y, 'in') assertEquals( cm, cm:same() )
  end
end

