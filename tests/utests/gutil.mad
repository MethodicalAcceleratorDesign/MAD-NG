function TestGutil:testIsSame()
  local t = {} ; for k in pairs(types) do t[#t+1] = k end
  local i, j = 1, 0
  local chk = \s =>
    if j >= #types[t[i]] then i, j = i+1, 1 else j = j+1 end
    return is_same(s, types[t[i]][1])
  end

  assertTypes( t, chk )
end

function TestGutil:testSame()
